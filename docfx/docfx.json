{
  "metadata": [
    {
      "src": [
        {
          "src": "../LocalPackage",
          "files": [
            "**/*.cs"
          ]
        }
      ],
      "allowCompilationErrors": true,
      "filter": "filterConfig.yml",
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "nf.unitylibs.managers.patchmanagement",
      "_appTitle": "nf.unitylibs.managers.patchmanagement",
      "_enableSearch": true,
      "pdf": false
    }
  }
}